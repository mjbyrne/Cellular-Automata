#include <iostream>
#include <vector>
using namespace std;

int main() {
   vector<vector<char>> boardVals;
   // 15x15 board
   
   for(int i = 0; i <= 25; i++){
     vector<char> temp;
     for(int j = 0; j <=75; j++){
       temp.push_back('O'); 
      }
      boardVals.push_back(temp);
   }
   
   for(int i = 0; i <= 25; i++){
     for(int j = 0; j <=75; j++){
       cout << boardVals[i][j];
     }
     cout << endl;
   }
   
   
   
}
/*  RULES
    alive cell if neighbors < 2        cell dies
    alive cell if 1 < neighbors < 4    cell stays alive
    alive cell if neighbors > 3        cell dies
    alive cell if neighbors  ==3       cell becomes alive
  */
