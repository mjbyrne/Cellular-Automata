#include <iostream>
#include <vector>
using namespace std;


template <class Value>
int neighborNumber(int yVal int xVal)







int main() {
   vector<vector<char>> boardVals;
   // 15x15 board
   
   for(int y = 0; y <= 25; y++){
     vector<char> temp;
     for(int x = 0; x <=75; x++){
       temp.push_back('O'); 
      }
      boardVals.push_back(temp);
   }
   
   for(int y = 0; y <= 25; y++){
     for(int x = 0; x <=75; x++){
       cout << boardVals[y][x];
     }
     cout << endl;
   }
   
   
   
}
/*  RULES
    alive cell if neighbors < 2        cell dies
    alive cell if 1 < neighbors < 4    cell stays alive
    alive cell if neighbors > 3        cell dies
    alive cell if neighbors  ==3       cell becomes alive
*/
