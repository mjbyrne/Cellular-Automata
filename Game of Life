
#include <iostream>
#include <vector>
using namespace std;

int main() {
   vector<vector<char>> boardVals;
   // 15x15 board
   
   for(int i = 0; i <= 25; i++){
     vector<char> temp;
     for(int j = 0; j <=75; j++){
       temp.push_back('O'); 
      }
      boardVals.push_back(temp);
   }
   
   for(int i = 0; i <= 25; i++){
     for(int j = 0; j <=75; j++){
       cout << boardVals[i][j];
     }
     cout << endl;
   }
   

}
